{
  "manifest_version": 3,
  "version": "1.0.9",
  "name": "PullPatrol",
  "short_name": "PullPatrol",
  "description": "Pull request management for OpenText Octane developers",
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "assets/background.js"
  },
  "permissions": [
    "background",
    "storage"
  ]
}
