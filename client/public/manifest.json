{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Polaris",
  "short_name": "Polaris",
  "description": "Polaris Chrome extension",
  "icons": {
    "128": "img/star128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "assets/background.js"
  },
  "permissions": [
    "background",
    "storage"
  ]
}
