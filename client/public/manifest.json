{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "PullPatrol",
  "short_name": "PullPatrol",
  "description": "PullPatrol Chrome extension",
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "assets/background.js"
  },
  "permissions": [
    "background",
    "storage"
  ]
}
